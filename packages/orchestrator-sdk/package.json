{
  "name": "@elemental/orchestrator-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "Orchestrator SDK for Elemental - AI agent orchestration library extending the core SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "default": "./dist/services/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js",
      "default": "./dist/runtime/index.js"
    },
    "./git": {
      "types": "./dist/git/index.d.ts",
      "import": "./dist/git/index.js",
      "default": "./dist/git/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js",
      "default": "./dist/testing/index.js"
    }
  },
  "scripts": {
    "build": "tsc && cp src/prompts/*.md dist/prompts/",
    "test": "bun test src",
    "test:node": "vitest run",
    "test:all": "pnpm run test && pnpm run test:node",
    "test:orchestration": "bun run src/cli/commands/test-orchestration.ts",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolco/elemental.git",
    "directory": "packages/orchestrator-sdk"
  },
  "author": "Toolco",
  "license": "AGPL-3.0-only",
  "keywords": [
    "elemental",
    "orchestrator",
    "sdk",
    "agents",
    "ai",
    "coordination",
    "claude"
  ],
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.23",
    "@elemental/core": "workspace:*",
    "@elemental/sdk": "workspace:*",
    "@elemental/storage": "workspace:*",
    "node-pty": "^1.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
