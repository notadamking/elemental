  "description": "A foundational library for building agent coordination systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "el": "./dist/bin/el.js",
    "elemental": "./dist/bin/el.js"
  },
  "scripts": {
